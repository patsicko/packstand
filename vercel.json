{
    "version": 2,
    "builds": [
      {
        "src": "packstand/wsgi.py", 
        "use": "@vercel/python",
        "config":{"maxlambdaSize":"15mb","runtime":"python3.10.12"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "packstand/wsgi.py" 
      }
    ]
  }
  